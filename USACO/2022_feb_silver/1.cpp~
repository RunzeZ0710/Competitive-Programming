#include <iostream>
#include <algorithm>
#include <string>
#include <vector>
#include <set>
#include <map>
#include <cmath>
#include <numeric>
#include <tuple>
#include <cstdio>
using namespace std;
using ll = long long;

int n; 
int ans[500] = {0};
bool visited[500] = {false};
vector<int> arr[500];

void dfs(int x){
	if(x == n) return;

}
int main(){
	ios_base::sync_with_stdio(0); cin.tie(0);
	cin >> n;
	int index = 0; 
	while(index < n){
		bool valid = true;
		for(int i = 0; i < n; i++){
			int a; cin >> a;
			if(valid) arr[index].push_back(a);
			if(a == index+1) valid = false;
		}
		index++;
	}
}
